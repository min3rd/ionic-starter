{
  "extends": [
    "development"
  ],
  "hints": {
    "meta-viewport": "off",
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "meta[name=color-scheme]",
          "meta[name=theme-color]"
        ]
      }
    ],
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off"
      }
    ],
    "apple-touch-icons": "off",
    "axe/structure": [
      "default",
      {
        "list": "off"
      }
    ],
    "axe/forms": [
      "default",
      {
        "label": "off"
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "button-name": "off"
      }
    ],
    "button-type": "off",
    "no-inline-styles": "off"
  }
}